#include<iostream>
using  namespace std;
void charfun()
{
 char a='a';
 cout<<"char ";
 cout<<a<<endl;
 }
 
 void shortfun()
{
 short a= 2;
 cout<<"short";
 cout<<a<<endl;
 }
 
 void intfun()
{
 int a= 28668;
 cout<<"int";
 cout<<a<<endl;
 }

void longfun()
{
 long a= 26865489;
 cout<<"long";
 cout<<a<<endl;
 }
 
 void floatfun()
{
 float a =23.3f;
 cout<<"float";
 cout<<a<<endl;
 }
 
 void doublefun()
{
 double a= 245.7698;
 cout<<"double ";
 cout<<a<<endl;
 }
 
 void longdoublefun()
{
 long double a = 248670854.57575768;
 cout<<"Long Double:";
 cout<<a<<endl;
 }
 
 void boolfun()
{
 bool a= true;
 cout<<"bool ";
 cout<<a<<endl;
 }
 
int main()
{
   charfun();
   shortfun();
   intfun();
   longfun();
   floatfun();
   doublefun();
   longdoublefun();
   boolfun();
  }
  
